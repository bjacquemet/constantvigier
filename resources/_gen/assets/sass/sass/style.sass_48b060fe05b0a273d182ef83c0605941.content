*{box-sizing:border-box}html{background-color:#0e111f;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased}body{background-color:#0e111f;color:#fff;font-size:14px;margin:0}body.home{background:url(/images/background1.jpg);background-size:contain;min-height:85vh;background-position:0 -133px;background-repeat:no-repeat}.page-header{background-color:#811825;padding:35px 0}.page-header.home{background-color:transparent}h1.site-title{z-index:99;text-align:center;font-family:cabin;font-weight:400;font-size:3.42em;line-height:1.21em;letter-spacing:.375em;width:400px;margin:0 auto;text-transform:uppercase;font-style:normal}h1.site-title a,h1.site-title a:focus,h1.site-title a:visited,h1.site-title a:active,h1.site-title a:link,h1.site-title a:hover{color:#fff;text-decoration:none}.menu{display:flex;flex-flow:row wrap;justify-content:space-around;max-width:780px;margin:auto;padding-left:0}.menu li{font-family:cabin;font-size:1.285em;text-transform:uppercase;letter-spacing:5px;font-weight:400;font-style:normal;line-height:1em;list-style-type:none;padding-top:1.3em;padding-bottom:1em}.menu li a{-webkit-transition:color .1s 0s ease-in-out;-moz-transition:color .1s 0s ease-in-out;-ms-transition:color .1s 0s ease-in-out;-o-transition:color .1s 0s ease-in-out;transition:color .1s 0s ease-in-out}.menu li a,.menu li a:visited,.menu li a:link{color:#dfd8b4;text-decoration:none}.menu li a:focus,.menu li a:hover,.menu li a:active{color:#fff}.menu li.active a,.menu li.active a:visited,.menu li.active a:link{color:#fff}.huge{font-family:cabin;font-size:6.95em;line-height:1.2em;text-transform:uppercase;letter-spacing:.12em;font-weight:400;font-style:normal;color:#fff;margin:0 auto;text-align:center;padding:180px 0 500px}.creations{list-style-type:none;display:flex;justify-content:center;align-content:space-around;flex-flow:row wrap;padding:0;margin:0}.creations li{width:200px;height:auto}footer .quote{font-family:source code pro;letter-spacing:.4px;line-height:2.1em;font-weight:500;background-color:#811825;color:#fff;text-align:center;padding:34px}footer .social-media{display:flex;justify-content:center;width:100%;margin-top:20px;fill:#fff;padding:70px 0}footer .social-media a{padding:0 10px}footer .social-media svg{-webkit-transition:fill .2s;-moz-transition:fill .2s;-ms-transition:fill .2s;-o-transition:fill .2s;transition:fill .2s}footer .social-media:hover svg{fill:#6e7078;-webkit-transition:fill .3s;-moz-transition:fill .3s;-ms-transition:fill .3s;-o-transition:fill .3s;transition:fill .3s}footer .social-media a:focus svg,footer .social-media a:active svg,footer .social-media a:hover svg{fill:#fff}.center{text-align:center}
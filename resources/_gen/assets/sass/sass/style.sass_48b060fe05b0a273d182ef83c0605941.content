*{box-sizing:border-box}html{background-color:#0e111f;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased}body{background-color:#0e111f;color:#fff;font-size:14px;margin:0}body.home{background:url(/images/background1.jpg);background-size:contain;min-height:85vh;background-position:0 -133px;background-repeat:no-repeat}body a,body a:hover,body a:focus,body a:active,body a:visited,body a:link{text-decoration:none}body a{-webkit-transition:color .1s 0s ease-in-out;-moz-transition:color .1s 0s ease-in-out;-ms-transition:color .1s 0s ease-in-out;-o-transition:color .1s 0s ease-in-out;transition:color .1s 0s ease-in-out}body button{text-transform:uppercase;font-size:1.1em;letter-spacing:.5px;border-radius:0;background-color:transparent;color:rgba(255,255,255,.9);border:2px solid rgba(255,255,255,.9);-webkit-transition:.2s background-color ease-in-out;-moz-transition:.2s background-color ease-in-out;-ms-transition:.2s background-color ease-in-out;-o-transition:.2s background-color ease-in-out;transition:.2s background-color ease-in-out;padding:1em 2.5em;cursor:pointer}body button:hover{color:#1d1d1d;background-color:rgba(255,255,255,.9)}body iframe{width:100%}body a,body a:visited,body a:link{color:#d8ccc0}body a:hover,body a:active,body a:focus{color:#fff}body figcaption h4{margin:5px;font-family:source code pro,sans-serif;font-weight:500;font-style:normal;font-size:.857em;line-height:1.357em}main{padding:75px 34px;max-width:1080px;margin:auto}.page-header{background-color:#811825;padding:35px 0}.page-header.home{background-color:transparent}h1.site-title{z-index:99;text-align:center;font-family:cabin,sans-serif;font-weight:400;font-size:3.42em;line-height:1.21em;letter-spacing:.375em;width:400px;margin:0 auto;text-transform:uppercase;font-style:normal}h1.site-title a,h1.site-title a:focus,h1.site-title a:visited,h1.site-title a:active,h1.site-title a:link,h1.site-title a:hover{color:#fff}.menu{display:flex;flex-flow:row wrap;justify-content:space-around;max-width:780px;margin:auto;padding-left:0}.menu li{font-family:cabin,sans-serif;font-size:1.285em;text-transform:uppercase;letter-spacing:5px;font-weight:400;font-style:normal;line-height:1em;list-style-type:none;padding-top:1.3em;padding-bottom:1em}.menu li a,.menu li a:visited,.menu li a:link{color:#dfd8b4}.menu li a:focus,.menu li a:hover,.menu li a:active{color:#fff}.menu li.active a{color:#fff;font-weight:500}form{font-family:source code pro,sans-serif;font-weight:500}form label{font-size:1em;line-height:1.5em;display:block}form input,form textarea{width:100%;padding:12px;margin:6px 0 24px;border:1px solid #ccc;background:#fafafa;color:#000;font-family:sans-serif;font-size:12px;line-height:normal;box-sizing:border-box;border-radius:2px}form textarea{min-height:100px}.huge{font-family:cabin,sans-serif;font-size:6.95em;line-height:1.2em;text-transform:uppercase;letter-spacing:.12em;font-weight:400;font-style:normal;color:#fff;margin:0 auto;text-align:center;padding:180px 0 500px}.container.biography{font-family:source code pro,sans-serif;font-weight:700;font-style:italic;font-size:1em;line-height:2.07em;text-align:center}.inline-img{text-align:center;display:block}.inline-img img{max-width:300px;margin-top:20px}ul.creations{list-style-type:none;display:grid;grid-template-columns:1fr 1fr 1fr;padding:0}ul.creations li{width:330px;height:auto;display:block;margin-bottom:40px}ul.creations li h2{font-family:abel,sans-serif;font-weight:400;line-height:1.2em;font-size:1.43em;text-transform:uppercase}ul.creations img{object-fit:cover;object-position:center;height:220px;width:300px}.container.creation{text-align:center;font-family:source code pro,sans-serif;font-weight:500;line-height:2.01em}.container.creation .back{text-align:left}.container.creation .back button{font-size:.85em;color:rgba(255,255,255,.6);border-color:rgba(255,255,255,.6)}.container.creation .back button:hover{background-color:rgba(255,255,255,.6);color:#1d1d1d}.container.creation .back button a,.container.creation .back button a:hover,.container.creation .back button a:visited,.container.creation .back button a:link,.container.creation .back button a:focus,.container.creation .back button a:active{color:inherit}.container.creation h1{text-align:center;font-family:abel,sans-serif;font-weight:700;font-size:2.285em;line-height:1.187em}.container.creation .youtube{margin-top:60px}.container.creation .gallery{margin-top:30px;display:flex;flex-flow:row wrap;justify-content:space-around}.container.creation .gallery img{object-fit:cover;object-position:center;margin:10px;max-height:250px;max-width:320px}footer .quote{font-family:source code pro,sans-serif;letter-spacing:.4px;line-height:2.1em;font-weight:500;background-color:#811825;color:#fff;text-align:center;padding:34px}footer .social-media{display:flex;justify-content:center;width:100%;margin-top:20px;padding:70px 0}footer .social-media a{padding:0 10px}footer .social-media svg{fill:#fff;-webkit-transition:fill .2s;-moz-transition:fill .2s;-ms-transition:fill .2s;-o-transition:fill .2s;transition:fill .2s}footer .social-media:hover svg{fill:#6e7078;-webkit-transition:fill .3s;-moz-transition:fill .3s;-ms-transition:fill .3s;-o-transition:fill .3s;transition:fill .3s}footer .social-media a:focus svg,footer .social-media a:active svg,footer .social-media a:hover svg{fill:#fff}.center{text-align:center}.left{text-align:left}.right{text-align:right}